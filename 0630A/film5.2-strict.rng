<?xml version="1.0" encoding="UTF-8"?>
<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://apple.com/itunes/importer" xmlns="http://relaxng.org/ns/structure/1.0" xmlns:ixa="http://apple.com/itunes/importer/alterations">
    <!--
      This is the main schema file from which ALL other itmsp schema files are derived.
    -->
    <start>
        <element name="package">
            <ref name="PackageType"/>
        </element>
    </start>
    <define name="PackageType">
        <attribute name="version">
            <ref name="PackageVersion"/>
        </attribute>
        <optional>
            <attribute name="generator">
                <ref name="String255"/>
            </attribute>
        </optional>
        <optional>
            <attribute name="generator_version">
                <ref name="String255"/>
            </attribute>
        </optional>
        <interleave>
            <optional>
                <element name="metadata_token">
                    <data type="string"/>
                </element>
            </optional>
            <optional>
                <element name="comments">
                    <data type="string"/>
                </element>
            </optional>
            <optional>
                <element name="language">
                    <ref name="LanguageType"/>
                </element>
            </optional>
            <element name="provider">
                <ref name="ProviderType"/>
            </element>
            <choice>
                <element name="album">
                    <ref name="Album"/>
                </element>
                <element name="tracks">
                    <ref name="TrackList"/>
                </element>
                <element name="assets">
                    <ref name="AssetList"/>
                </element>
                <element name="video">
                    <ref name="Video"/>
                </element>
                <element name="artist">
                    <ref name="ArtistType"/>
                </element>
                <element name="itunes_extra">
                    <ref name="ItunesExtra"/>
                </element>
                <element name="clone_assets">
                    <ref name="CloneAssetList"/>
                </element>
            </choice>
        </interleave>
    </define>
    <define name="PackageVersion">
        <data type="token">
            <param name="pattern">film\d+(\.\d+)*</param>
        </data>
    </define>
    <define name="AssetList">
        <optional>
            <!-- container_id is required for tv. -->
            <attribute name="container_id">
                <ref name="VendorIdType"/>
            </attribute>
        </optional>
        <optional>
            <attribute name="media_type">
                <choice>
                    <value>video</value>
                    <!-- playlist is for film or tv only. -->
                    <value>playlist</value>
                    <!-- for media archive files -->
                    <value>extra_video_item</value>
                    <!-- for extras2.0 -->
                    <value>gallery_item_video</value>
                    <value>gallery_item_image</value>
                    <value>gallery_item_audio</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <!-- vendor_id is required for film but not for tv. -->
            <attribute name="vendor_id">
                <ref name="VendorIdType"/>
            </attribute>
        </optional>
        <interleave>
            <oneOrMore>
                <element name="asset">
                    <ref name="AssetItem"/>
                </element>
            </oneOrMore>
            <optional>
                <element name="accessibility_info">
                    <ref name="AccessibilityInfo"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="CloneAssetList">
        <optional>
            <!-- container_id is required for tv. -->
            <attribute name="container_id">
                <ref name="VendorIdType"/>
            </attribute>
        </optional>
        <optional>
            <attribute name="media_type">
                <choice>
                    <value>video</value>
                </choice>
            </attribute>
        </optional>
        <attribute name="origin_provider">
            <ref name="ProviderType"/>
        </attribute>
        <attribute name="origin_vendor_id">
            <ref name="VendorIdType"/>
        </attribute>
        <interleave>
            <optional>
                <attribute name="target_vendor_id">
                    <ref name="VendorIdType"/>
                </attribute>
            </optional>
            <zeroOrMore>
                <element name="clone_asset">
                    <ref name="CloneAssetItem"/>
                </element>
            </zeroOrMore>
            <optional>
                <element name="clone_chapters">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="Video">
        <interleave>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <optional>
                <element name="original_spoken_locale">
                    <ref name="LanguageType"/>
                </element>
            </optional>
            <optional>
                <element name="phonetic_title">
                    <ref name="NonEmptyTokenString"/>
                </element>
            </optional>
            <optional>
                <element name="vendor_offer_code">
                    <ref name="VendorOfferCode"/>
                </element>
            </optional>
            <optional>
                <element name="publishers">
                    <ref name="PublisherList"/>
                </element>
            </optional>
            <optional>
                <element name="volume_number">
                    <ref name="FormattedPositiveInteger"/>
                </element>
            </optional>
            <optional>
                <element name="artists">
                    <ref name="ArtistList"/>
                </element>
            </optional>
            <optional>
                <element name="artwork_files">
                    <ref name="ArtworkFilesList"/>
                </element>
            </optional>
            <optional>
                <element name="copyright_cline">
                    <ref name="Copyright"/>
                </element>
            </optional>
            <optional>
                <element name="copyright_pline">
                    <ref name="Copyright"/>
                </element>
            </optional>
            <optional>
                <element name="explicit_content">
                    <ref name="ParentalAdvisoryType"/>
                </element>
            </optional>
            <optional>
                <element name="grid">
                    <ref name="Grid"/>
                </element>
            </optional>
            <optional>
                <element name="isrc">
                    <ref name="Isrc"/>
                </element>
            </optional>
            <optional>
                <element name="eidr">
                    <ref name="Eidr"/>
                </element>
            </optional>
            <optional>
                <element name="preorder_type">
                    <ref name="PreOrder"/>
                </element>
            </optional>
            <optional>
                <element name="preview">
                    <ref name="VideoPreviewType"/>
                </element>
            </optional>
            <optional>
                <element name="release_date">
                    <ref name="FormattedDate"/>
                </element>
            </optional>
            <!-- nuglov : LocalizableText continues to exist only to support 4.x metadata lookup beans. It is completely altered out for input. -->
            <zeroOrMore>
                <element name="title">
                    <text/>
                </element>
            </zeroOrMore>
            <!-- nuglov : LocalizableText continues to exist only to support 4.x metadata lookup beans. It is completely altered out for input. -->
            <zeroOrMore>
                <element name="title_version">
                    <text/>
                </element>
            </zeroOrMore>
            <optional>
                <element name="track_number">
                    <ref name="FormattedPositiveInteger"/>
                </element>
            </optional>
            <optional>
                <element name="upc">
                    <ref name="Upc"/>
                </element>
            </optional>
            <optional>
                <element name="vendor_id">
                    <ref name="VendorIdType"/>
                </element>
            </optional>
            <optional>
                <element name="xid">
                    <ref name="Xid"/>
                </element>
            </optional>
            <optional>
                <element name="assets">
                    <ref name="AssetList"/>
                </element>
            </optional>
            <optional>
                <element name="clone_assets">
                    <ref name="CloneAssetList"/>
                </element>
            </optional>
            <optional>
                <element name="share_assets">
                    <ref name="ShareAssets"/>
                </element>
            </optional>
            <optional>
                <element name="genres">
                    <ref name="GenreList"/>
                </element>
            </optional>
            <optional>
                <element name="label_name">
                    <ref name="LabelNameType"/>
                </element>
            </optional>
            <optional>
                <element name="products">
                    <ref name="ProductList"/>
                </element>
            </optional>
            <!-- Begin film and tv elements -->
            <optional>
                <element name="subtype">
                    <choice>
                        <!-- film subtypes -->
                        <value>concert</value>
                        <value>short</value>
                        <value>feature</value>
                        <value>speciality</value>
                        <!-- tv subtypes -->
                        <value>episode</value>
                        <value>episodebonus</value>
                        <value>seasonbonus</value>
                    </choice>
                </element>
            </optional>
            <optional>
                <element name="ratings">
                    <oneOrMore>
                        <choice>
                            <element name="rating">
                                <ref name="FilmRating"/>
                            </element>
                            <element name="advisory">
                                <ref name="VideoAdvisory"/>
                            </element>
                        </choice>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="type">
                    <data type="string">
                        <param name="pattern">([Ff][Ii][Ll][Mm])|([Tt][Vv])</param>
                    </data>
                </element>
            </optional>
            <optional>
                <element name="cue_sheet">
                    <ref name="CueSheet"/>
                </element>
            </optional>
            <optional>
                <element name="locales">
                    <oneOrMore>
                        <element name="locale">
                            <ref name="VideoLocale"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="regions">
                    <oneOrMore>
                        <element name="region">
                            <ref name="VideoRegion"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="chapters">
                    <ref name="ChapterList"/>
                </element>
            </optional>
            <optional>
                <element name="accessibility_info">
                    <ref name="AccessibilityInfo"/>
                </element>
            </optional>
            <optional>
                <element name="studio_release_title">
                    <ref name="StudioReleaseTitle"/>
                </element>
            </optional>
            <optional>
                <element name="previews">
                    <ref name="PreviewDefinitionList"/>
                </element>
            </optional>
            <!-- End film and tv elements -->
            <!-- Begin rentals properties -->
            <optional>
                <element name="theatrical_release_date">
                    <ref name="FormattedDate"/>
                </element>
            </optional>
            <!-- End rentals properties -->
            <optional>
                <choice>
                    <!-- "description" replaces long_description in the 3.3.2 TV spec -->
                    <element name="long_description">
                        <ref name="String4000"/>
                    </element>
                    <element name="synopsis">
                        <ref name="String4000"/>
                    </element>
                </choice>
            </optional>
            <!-- End tv-specific properties -->
            <!-- Begin film-specific properties -->
            <optional>
                <element name="production_company">
                    <ref name="String255"/>
                </element>
            </optional>
            <optional>
                <element name="isan">
                    <ref name="String50"/>
                </element>
            </optional>
            <optional>
                <element name="country">
                    <ref name="Territory"/>
                </element>
            </optional>
            <optional>
                <element name="cast">
                    <oneOrMore>
                        <choice>
                            <element name="actor">
                                <ref name="MovieParticipant"/>
                            </element>
                            <element name="cast_member">
                                <ref name="LocalizableMovieParticipant"/>
                            </element>
                        </choice>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="crew">
                    <oneOrMore>
                        <choice>
                            <element name="member">
                                <ref name="MovieParticipant"/>
                            </element>
                            <element name="crew_member">
                                <ref name="LocalizableMovieParticipant"/>
                            </element>
                        </choice>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="bonus_materials">
                    <ref name="BonusMaterialList"/>
                </element>
            </optional>
            <!-- End film-specific properties -->
            <optional>
                <element name="extra">
                    <ref name="Extra"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="AccessibilityInfo">
        <interleave>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <zeroOrMore>
                <element name="accessibility">
                    <ref name="AccessibilityDetail"/>
                </element>
            </zeroOrMore>
        </interleave>
    </define>
    <define name="AccessibilityDetail">
        <attribute name="role">
            <ref name="AccessibilityRole"/>
        </attribute>
        <attribute name="available">
            <data type="boolean"/>
        </attribute>
        <optional>
            <attribute name="locale">
                <ref name="Locale"/>
            </attribute>
        </optional>
        <optional>
            <attribute name="reason_code">
                <ref name="AccessibilityReasonCode"/>
            </attribute>
        </optional>
    </define>
    <define name="AccessibilityRole">
        <choice>
            <value>captions</value>
        </choice>
    </define>
    <define name="AccessibilityReasonCode">
        <data type="string"/>
    </define>
    <!-- Begin film and tv-specific definitions -->
    <define name="ShareAssets">
        <interleave>
            <optional>
                <!-- provider short name is optional and implicitly the current -->
                <attribute name="provider">
                    <ref name="ProviderType"/>
                </attribute>
            </optional>
            <optional>
                <attribute name="vendor_id">
                    <ref name="VendorIdType"/>
                </attribute>
            </optional>
            <optional>
                <attribute name="apple_id">
                    <ref name="FormattedPositiveInteger"/>
                </attribute>
            </optional>
        </interleave>
    </define>
    <define name="VideoLocale">
        <attribute name="name">
            <ref name="LanguageType"/>
        </attribute>
        <interleave>
            <optional>
                <element name="designated_fallback">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="title">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="title_version">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="short_description">
                    <ref name="String255"/>
                </element>
            </optional>
            <optional>
                <choice>
                    <element name="long_description">
                        <text/>
                    </element>
                    <element name="synopsis">
                        <ref name="String4000"/>
                    </element>
                </choice>
            </optional>
            <optional>
                <element name="phonetic_title">
                    <text/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="VideoRegion">
        <interleave>
            <element name="territory">
                <ref name="Territory"/>
            </element>
            <optional>
                <element name="theatrical_release_date">
                    <ref name="FormattedDate"/>
                </element>
            </optional>
            <optional>
                <element name="release_date">
                    <ref name="FormattedDate"/>
                </element>
            </optional>
            <optional>
                <element name="copyright_pline">
                    <ref name="Copyright"/>
                </element>
            </optional>
            <optional>
                <element name="copyright_cline">
                    <ref name="Copyright"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="CueSheet">
        <zeroOrMore>
            <element name="cue">
                <ref name="Cue"/>
            </element>
        </zeroOrMore>
    </define>
    <define name="Cue">
        <interleave>
            <element name="sequence_number">
                <ref name="FormattedPositiveInteger"/>
            </element>
            <element name="time">
                <ref name="CueTime"/>
            </element>
            <element name="title">
                <text/>
            </element>
            <element name="artists">
                <ref name="ArtistList"/>
            </element>
            <optional>
                <element name="isrc">
                    <ref name="Isrc"/>
                </element>
            </optional>
            <optional>
                <element name="iswc">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="copyright_cline">
                    <ref name="Copyright"/>
                </element>
            </optional>
            <optional>
                <element name="copyright_pline">
                    <ref name="Copyright"/>
                </element>
            </optional>
            <optional>
                <element name="usage">
                    <ref name="CueUsage"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="CueUsage">
        <data type="token">
            <param name="pattern">([Bb][Aa][Cc][Kk][Gg][Rr][Oo][Uu][Nn][Dd])|([Ff][Ee][Aa][Tt][Uu][Rr][Ee][Dd])|([Ss][Ii][Gg][Nn][Aa][Tt][Uu][Rr][Ee])</param>
        </data>
    </define>
    <define name="CueTime">
        <interleave>
            <optional>
                <element name="start_timecode">
                    <ref name="FormattedTime"/>
                </element>
            </optional>
            <element name="duration">
                <ref name="FormattedTime"/>
            </element>
        </interleave>
    </define>
    <!-- VideoAdvisory is only supported by TV -->
    <define name="VideoAdvisory">
        <attribute name="system">
            <choice>
                <value>us-cable</value>
                <value>us-tv</value>
                <value>au-tv</value>
            </choice>
        </attribute>
        <!-- code-enforced: code is being rolled out and preferred over name, but we're not ready to require it yet -->
        <optional>
            <attribute name="code">
                <ref name="NonEmptyTokenString"/>
            </attribute>
        </optional>
        <choice>
            <value>A</value>
            <value>AC</value>
            <value>AL</value>
            <value>B</value>
            <value>BN</value>
            <value>D</value>
            <value>FV</value>
            <value>GL</value>
            <value>GV</value>
            <value>H</value>
            <value>L</value>
            <value>M</value>
            <value>MV</value>
            <value>N</value>
            <value>RP</value>
            <value>S</value>
            <value>SN</value>
            <value>SC</value>
            <value>V</value>
            <value>W</value>
            <value/>
            <!-- code-enforced: code is being rolled out and preferred over name -->
        </choice>
    </define>
    <!-- FilmRating is supported by Film and TV -->
    <define name="FilmRating">
        <attribute name="system">
            <ref name="NonEmptyTokenString"/>
        </attribute>
        <!-- code-enforced: code is being rolled out and preferred over name, but we're not ready to require it yet -->
        <optional>
            <attribute name="code">
                <ref name="NonEmptyTokenString"/>
            </attribute>
        </optional>
        <!-- code-enforced: reason is required for Film but not TV -->
        <optional>
            <attribute name="reason">
                <data type="token"/>
            </attribute>
        </optional>
        <data type="token"/>
        <!-- code-enforced: code is being rolled out and preferred over name -->
    </define>
    <define name="MovieParticipant">
        <attribute name="name">
            <ref name="String250"/>
        </attribute>
        <attribute name="display">
            <ref name="String250"/>
        </attribute>
        <optional>
            <attribute name="billing">
                <!-- Case-insensitive enumeration of: ordered, top -->
                <data type="token">
                    <param name="pattern">([Oo][Rr][Dd][Ee][Rr][Ee][Dd])|([Tt][Oo][Pp])</param>
                </data>
            </attribute>
        </optional>
        <ref name="NonEmptyTokenString"/>
    </define>
    <define name="LocalizableMovieParticipant">
        <optional>
            <attribute name="billing">
                <!-- Case-insensitive enumeration of: ordered, top -->
                <data type="token">
                    <param name="pattern">([Oo][Rr][Dd][Ee][Rr][Ee][Dd])|([Tt][Oo][Pp])</param>
                </data>
            </attribute>
        </optional>
        <!-- New style cast & crew translations -->
        <interleave>
            <optional>
                <element name="apple_id">
                    <ref name="FormattedPositiveInteger"/>
                </element>
            </optional>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <optional>
                <element name="display_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <choice>
                    <!-- Character_name deprecated in 5.2, replaced with characters block-->
                    <element name="character_name">
                        <ref name="String250"/>
                    </element>
                    <element name="characters">
                        <ref name="Characters"/>
                    </element>
                </choice>
            </optional>
            <optional>
                <element name="phonetic_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <element name="sort_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <element name="locales">
                    <oneOrMore>
                        <element name="locale">
                            <ref name="MovieParticipantLocale"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="roles">
                    <oneOrMore>
                        <element name="role">
                            <ref name="MovieRole"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="description">
                    <ref name="Description"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="MovieParticipantLocale">
        <attribute name="name">
            <ref name="Locale"/>
        </attribute>
        <interleave>
            <optional>
                <element name="designated_fallback">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="display_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <element name="character_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <element name="phonetic_name">
                    <ref name="String250"/>
                </element>
            </optional>
            <optional>
                <element name="description">
                    <ref name="Description"/>
                </element>
            </optional>
        </interleave>
    </define>
    <!-- End film and tv-specific definitions -->
    <define name="AssetItem">
        <optional>
            <attribute name="type">
                <choice>
                    <value>full</value>
                    <value>preview</value>
                    <value>artwork</value>
                    <!-- head and tail are only allowed for tv. -->
                    <value>head</value>
                    <value>tail</value>
                    <!-- timeline is for extras2.0 -->
                    <value>timeline</value>
                </choice>
            </attribute>
        </optional>
        <!-- enabled is only for tv. -->
        <!--
        <optional>
            <attribute name="enabled">
                <ref name="FormattedBoolean"/>
            </attribute>
        </optional>
        -->
        <!-- display_target is only for extras2.0 -->
        <optional>
            <attribute name="display_target">
                <ref name="ItunesExtraDisplayTargetEnum"/>
            </attribute>
        </optional>
        <interleave>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <optional>
                <element name="territories">
                    <zeroOrMore>
                        <element name="territory">
                            <ref name="Territory"/>
                        </element>
                    </zeroOrMore>
                </element>
            </optional>
            <!-- only one data_file element is allowed at this level for tv. -->
            <oneOrMore>
                <element name="data_file">
                    <ref name="AssetFile"/>
                </element>
            </oneOrMore>
            <!-- chapters can be sent under the full asset in an asset-only update -->
            <optional>
                <element name="chapters">
                    <ref name="ChapterList"/>
                </element>
            </optional>
            <optional>
                <element name="preview_image_time">
                    <ref name="PreviewImageTime"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="PreviewImageTime">
        <optional>
            <attribute name="format">
                <ref name="TimecodeFormat"/>
            </attribute>
        </optional>
        <ref name="FormattedTime"/>
    </define>
    <define name="CloneAssetItem">
        <optional>
            <attribute name="type">
                <choice>
                    <value>full</value>
                    <value>preview</value>
                    <value>artwork</value>
                </choice>
            </attribute>
        </optional>
        <interleave>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <optional>
                <element name="territories">
                    <zeroOrMore>
                        <element name="territory">
                            <ref name="Territory"/>
                        </element>
                    </zeroOrMore>
                </element>
            </optional>
            <zeroOrMore>
                <element name="clone_data_file">
                    <ref name="CloneAssetFile"/>
                </element>
            </zeroOrMore>
        </interleave>
    </define>
    <define name="Album">
        <interleave>
            <optional>
                <element name="read_only_info">
                    <ref name="ReadOnlyInfo"/>
                </element>
            </optional>
            <optional>
                <element name="galleries">
                    <ref name="GalleryList"/>
                </element>
