                <element name="transition">
                    <text/>
                    <!-- TODO enumerate types -->
                </element>
            </optional>
            <optional>
                <element name="show_play_all">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="inherit_background_image">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="inherit_background_audio">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="inherit_background_video">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="navnodes">
                    <ref name="NavnodeList"/>
                </element>
            </optional>
            <optional>
                <element name="preview_label">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="movie_label">
                    <text/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="Characters">
        <zeroOrMore>
            <element name="character">
                <ref name="Character"/>
            </element>
        </zeroOrMore>
    </define>
    <define name="Character">
        <interleave>
            <element name="character_name">
                <ref name="String250"/>
            </element>
            <element name="reference_id">
                <ref name="NonEmptyTokenString"/>
            </element>
            <optional>
                <element name="character_note">
                    <ref name="String4000"/>
                </element>
            </optional>
            <optional>
                <choice>
                    <element name="character_image">
                        <element name="data_file">
                            <ref name="AssetFile"/>
                        </element>
                    </element>
                    <element name="clone_character_image">
                        <ref name="FormattedBoolean"/>
                    </element>
                </choice>
            </optional>
            <optional>
                <element name="locales">
                    <ref name="CharacterLocales"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="CharacterLocales">
        <oneOrMore>
            <element name="locale">
                <interleave>
                    <optional>
                        <element name="designated_fallback">
                            <ref name="FormattedBoolean"/>
                        </element>
                    </optional>
                    <optional>
                        <element name="character_name">
                            <ref name="String250"/>
                        </element>
                    </optional>
                    <optional>
                        <element name="character_note">
                            <ref name="String4000"/>
                        </element>
                    </optional>
                </interleave>
                <attribute name="name">
                    <ref name="LanguageType"/>
                </attribute>
            </element>
        </oneOrMore>
    </define>
    <define name="Description">
        <attribute name="format">
            <choice>
                <value>html</value>
                <value>plain</value>
            </choice>
        </attribute>
        <text/>
    </define>
    <define name="SceneGroupCollection">
        <interleave>
            <optional>
                <element name="timecode_format">
                    <ref name="TimecodeFormat"/>
                </element>
            </optional>
            <zeroOrMore>
                <element name="scene_group">
                    <ref name="SceneGroup"/>
                </element>
            </zeroOrMore>
        </interleave>
    </define>
    <define name="SceneGroup">
        <interleave>
            <element name="vendor_id">
                <ref name="VendorIdType"/>
            </element>
            <element name="title">
                <ref name="NonEmptyTokenString"/>
            </element>
            <optional>
                <element name="locales">
                    <oneOrMore>
                        <element name="locale">
                            <ref name="SceneGroupLocale"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <element name="scene_links">
                <ref name="SceneLinkCollection"/>
            </element>
        </interleave>
    </define>
    <define name="SceneGroupLocale">
        <attribute name="name">
            <ref name="LanguageType"/>
        </attribute>
        <interleave>
            <optional>
                <element name="designated_fallback">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="title">
                    <text/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="SceneLinkCollection">
        <oneOrMore>
            <element name="scene_link">
                <ref name="SceneLink"/>
            </element>
        </oneOrMore>
    </define>
    <define name="SceneLink">
        <attribute name="vendor_id">
            <ref name="VendorIdType"/>
        </attribute>
        <optional>
            <attribute name="key_scene">
                <ref name="FormattedBoolean"/>
            </attribute>
        </optional>
    </define>
    <define name="SceneCollection">
        <zeroOrMore>
            <element name="scene">
                <ref name="Scene"/>
            </element>
        </zeroOrMore>
    </define>
    <define name="Scene">
        <interleave>
            <element name="vendor_id">
                <ref name="VendorIdType"/>
            </element>
            <element name="title">
                <ref name="NonEmptyTokenString"/>
            </element>
            <element name="media_reference">
                <ref name="MediaReference"/>
            </element>
            <optional>
                <element name="description">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="alt_description">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="locales">
                    <oneOrMore>
                        <element name="locale">
                            <ref name="SceneLocale"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
            <optional>
                <element name="assets">
                    <ref name="AssetList"/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="SceneLocale">
        <attribute name="name">
            <ref name="LanguageType"/>
        </attribute>
        <interleave>
            <optional>
                <element name="designated_fallback">
                    <ref name="FormattedBoolean"/>
                </element>
            </optional>
            <optional>
                <element name="title">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="description">
                    <text/>
                </element>
            </optional>
            <optional>
                <element name="alt_description">
                    <text/>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="MediaReference">
        <attribute name="start">
            <ref name="FormattedTime"/>
        </attribute>
        <attribute name="end">
            <ref name="FormattedTime"/>
        </attribute>
    </define>
    <define name="ProviderReviewer">
        <attribute name="reviewer_id">
            <text/>
        </attribute>
    </define>
    <define name="ProviderReview">
        <interleave>
            <element name="review_date">
                <ref name="FormattedDate"/>
            </element>
            <element name="reviewer">
                <ref name="ProviderReviewer"/>
            </element>
            <optional>
                <element name="review_problems">
                    <oneOrMore>
                        <element name="review_problem">
                            <ref name="ReviewProblem"/>
                        </element>
                    </oneOrMore>
                </element>
            </optional>
        </interleave>
    </define>
    <define name="ReviewProblemCode">
        <attribute name="value">
            <text/>
        </attribute>
    </define>
    <define name="ReviewProblem">
        <interleave>
            <element name="review_problem_codes">
                <oneOrMore>
                    <element name="review_problem_code">
                        <ref name="ReviewProblemCode"/>
                    </element>
                </oneOrMore>
            </element>
            <optional>
                <element name="review_problem_note">
                    <text/>
                </element>
            </optional>
            <element name="review_components">
                <oneOrMore>
                    <element name="review_component">
                        <attribute name="type">
                            <data type="token"/>
                        </attribute>
                        <zeroOrMore>
                            <element name="review_component_item">
                                <ref name="ReviewComponentItem"/>
                            </element>
                        </zeroOrMore>
                    </element>
                </oneOrMore>
            </element>
        </interleave>
    </define>
    <define name="ReviewComponentItem">
        <optional>
            <attribute name="locale">
                <ref name="LanguageType"/>
            </attribute>
        </optional>
        <attribute name="type">
            <text/>
        </attribute>
    </define>
</grammar>
